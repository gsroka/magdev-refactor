    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
